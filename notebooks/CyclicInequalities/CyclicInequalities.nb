Notebook[{
Cell["Cyclic Inequalities: Toolkit and Automatic Proof", "Title"],

Cell["This notebook provides utilities for cyclic sums and automatically \
proves the inequality:\nSum[u[i]*(u[i-1]^2 - 4*u[i]^2 + 3*u[i+1]^2), \
{i,1,N}] <= 0 for u[i] >= 0 with cyclic boundary conditions u[0]=u[N], \
u[N+1]=u[1].", "Text"],

Cell["Initialization: Cyclic tools", "Section"],

Cell[BoxData[
 RowBox[{
  RowBox[{"ClearAll", "[", 
   RowBox[{
   "VectorCyclicSum", ",", "CyclicSum", ",", "CyclicAverage", ",", 
    "CyclicCanonicalize", ",", "FindEdgeSOSAnsatz", ",", 
    "ProveExampleInequality"}], "]"}], ";", "\[IndentingNewLine]", 
  "\[IndentingNewLine]", 
  RowBox[{"(*", " ", 
   RowBox[{
   "Vectorized", " ", "builder", " ", "for", " ", "local", " ", "terms", " ", 
    "of", " ", "the", " ", "form", " ", 
    RowBox[{"f", 
     RowBox[{"(", 
      RowBox[{
      "u_i", ",", " ", "u_", 
       RowBox[{"{", 
        RowBox[{"i", "-", "1"}], "}"}], ",", " ", "u_", 
       RowBox[{"{", 
        RowBox[{"i", "+", "1"}], "}"}]}], ")"}]}]}], " ", "*)"}], 
  "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"VectorCyclicSum", "[", 
     RowBox[{"f_", ",", "u_List"}], "]"}], ":=", 
    RowBox[{"Module", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
        RowBox[{"left", "=", 
         RowBox[{"RotateLeft", "[", "u", "]"}]}], ",", 
        RowBox[{"right", "=", 
         RowBox[{"RotateRight", "[", "u", "]"}]}]}], "}"}], ",", 
      "\[IndentingNewLine]", 
      RowBox[{"Total", "[", 
       RowBox[{"MapThread", "[", 
        RowBox[{"f", ",", 
         RowBox[{"{", 
          RowBox[{"u", ",", "right", ",", "left"}], "}"}]}], "]"}], "]"}]}], 
     "\[IndentingNewLine]", "]"}]}], ";"}], "\[IndentingNewLine]", 
  "\[IndentingNewLine]", 
  RowBox[{"(*", " ", 
   RowBox[{
   "Symbolic", " ", "cyclic", " ", "sum", " ", "over", " ", "indices", " ", 
    "with", " ", 
    RowBox[{"u", "[", "0", "]"}], "=", 
    RowBox[{"u", "[", "n", "]"}], ",", " ", 
    RowBox[{
     RowBox[{"u", "[", 
      RowBox[{"n", "+", "1"}], "]"}], "=", 
     RowBox[{"u", "[", "1", "]"}]}]}], " ", "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"CyclicSum", "[", 
     RowBox[{"term_", ",", "n_Integer"}], "]"}], ":=", 
    RowBox[{"Module", "[", 
     RowBox[{
      RowBox[{"{", "ii", "}"}], ",", "\[IndentingNewLine]", 
      RowBox[{"Total", "[", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"term", "/.", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{
             RowBox[{"u", "[", "0", "]"}], "->", 
             RowBox[{"u", "[", "n", "]"}]}], ",", 
            RowBox[{
             RowBox[{"u", "[", 
              RowBox[{"n", "+", "1"}], "]"}], "->", 
             RowBox[{"u", "[", "1", "]"}]}], ",", 
            RowBox[{"i", "->", "ii"}]}], "}"}]}], ",", 
         RowBox[{"{", 
          RowBox[{"ii", ",", "1", ",", "n"}], "}"}]}], "]"}], "]"}]}], 
     "\[IndentingNewLine]", "]"}]}], ";"}], "\[IndentingNewLine]", 
  "\[IndentingNewLine]", 
  RowBox[{"(*", " ", 
   RowBox[{
   "Cyclic", " ", "average", " ", "and", " ", "canonicalization", " ", 
    "under", " ", "rotations"}], " ", "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"CyclicAverage", "[", 
     RowBox[{"expr_", ",", "vars_List"}], "]"}], ":=", 
    RowBox[{"Mean", "[", 
     RowBox[{"Table", "[", 
      RowBox[{
       RowBox[{"expr", "/.", 
        RowBox[{"Thread", "[", 
         RowBox[{"vars", "->", 
          RowBox[{"RotateLeft", "[", 
           RowBox[{"vars", ",", "k"}], "]"}]}], "]"}]}], ",", 
       RowBox[{"{", 
        RowBox[{"k", ",", "0", ",", 
         RowBox[{
          RowBox[{"Length", "[", "vars", "]"}], "-", "1"}]}], "}"}]}], "]"}],
      "]"}]}], ";"}], "\[IndentingNewLine]", "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"CyclicCanonicalize", "[", 
     RowBox[{"expr_", ",", "vars_List"}], "]"}], ":=", 
    RowBox[{"Module", "[", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"rots", "=", 
        RowBox[{"Table", "[", 
         RowBox[{
          RowBox[{"expr", "/.", 
           RowBox[{"Thread", "[", 
            RowBox[{"vars", "->", 
             RowBox[{"RotateLeft", "[", 
              RowBox[{"vars", ",", "k"}], "]"}]}], "]"}]}], ",", 
          RowBox[{"{", 
           RowBox[{"k", ",", "0", ",", 
            RowBox[{
             RowBox[{"Length", "[", "vars", "]"}], "-", "1"}]}], "}"}]}], 
         "]"}]}], "}"}], ",", "\[IndentingNewLine]", 
      RowBox[{"First", "[", 
       RowBox[{"MinimalBy", "[", 
        RowBox[{"rots", ",", "ToString"}], "]"}], "]"}]}], 
     "\[IndentingNewLine]", "]"}]}], ";"}], "\[IndentingNewLine]", 
  "\[IndentingNewLine]", 
  RowBox[{"(*", " ", 
   RowBox[{
   "Discover", " ", "edge", "-", "wise", " ", "SOS", " ", "representation", 
    " ", "for", " ", "a", " ", "two", "-", "variable", " ", "cubic", " ", 
    "template", "\[IndentingNewLine]", "       ", "a", " ", 
    RowBox[{"x", "^", "3"}], " ", "+", " ", "b", " ", 
    RowBox[{"y", "^", "3"}], " ", "+", " ", "p", " ", 
    RowBox[{"x", "^", "2"}], " ", "y", " ", "+", " ", "q", " ", "x", " ", 
    RowBox[{"y", "^", "2"}], " ", "==", " ", 
    RowBox[{
     RowBox[{
      RowBox[{"(", 
       RowBox[{"x", " ", "-", " ", "y"}], ")"}], "^", "2"}], " ", 
     RowBox[{"(", 
      RowBox[{
       RowBox[{"c", " ", "x"}], " ", "+", " ", 
       RowBox[{"d", " ", "y"}]}], ")"}]}]}], " ", "*)"}], 
  "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"FindEdgeSOSAnsatz", "[", 
     RowBox[{"p_", ",", "q_"}], "]"}], ":=", 
    RowBox[{"Module", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
       "x", ",", "y", ",", "a", ",", "b", ",", "c", ",", "d", ",", "poly", 
        ",", "eqs"}], "}"}], ",", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"poly", "=", 
        RowBox[{
         RowBox[{"a", " ", 
          RowBox[{"x", "^", "3"}]}], "+", 
         RowBox[{"b", " ", 
          RowBox[{"y", "^", "3"}]}], "+", 
         RowBox[{"p", " ", 
          RowBox[{"x", "^", "2"}], " ", "y"}], "+", 
         RowBox[{"q", " ", "x", " ", 
          RowBox[{"y", "^", "2"}]}], "-", 
         RowBox[{
          RowBox[{
           RowBox[{"(", 
            RowBox[{"x", "-", "y"}], ")"}], "^", "2"}], 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"c", " ", "x"}], "+", 
            RowBox[{"d", " ", "y"}]}], ")"}]}]}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"eqs", "=", 
        RowBox[{"Thread", "[", 
         RowBox[{
          RowBox[{"Flatten", "[", 
           RowBox[{"CoefficientList", "[", 
            RowBox[{
             RowBox[{"Expand", "[", "poly", "]"}], ",", 
             RowBox[{"{", 
              RowBox[{"x", ",", "y"}], "}"}]}], "]"}], "]"}], "==", "0"}], 
         "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"Solve", "[", 
        RowBox[{"eqs", ",", 
         RowBox[{"{", 
          RowBox[{"a", ",", "b", ",", "c", ",", "d"}], "}"}], ",", "Reals"}],
         "]"}]}]}], "\[IndentingNewLine]", "]"}]}], ";"}], 
  "\[IndentingNewLine]", "\[IndentingNewLine]", 
  RowBox[{"(*", " ", 
   RowBox[{"Prove", ":", " ", 
    RowBox[{"Sum", "[", 
     RowBox[{
      RowBox[{"u_i", " ", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"u_", 
          RowBox[{"{", 
           RowBox[{"i", "-", "1"}], "}"}], "^", "2"}], " ", "-", " ", 
         RowBox[{"4", " ", 
          RowBox[{"u_i", "^", "2"}]}], " ", "+", " ", 
         RowBox[{"3", " ", "u_", 
          RowBox[{"{", 
           RowBox[{"i", "+", "1"}], "}"}], "^", "2"}]}], ")"}]}], "]"}], " ", 
    "<=", " ", "0", " ", "for", " ", "u_i", " ", ">=", " ", "0"}], " ", 
   "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"ProveExampleInequality", "[", "N_Integer", "]"}], ":=", 
   RowBox[{"Module", "[", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
      "u", ",", "left", ",", "right", ",", "S", ",", "sol", ",", "a", ",", 
       "b", ",", "c", ",", "d", ",", "P", ",", "idOK", ",", "nonnegOK", ",", 
       "concl"}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"u", "=", 
       RowBox[{"Array", "[", 
        RowBox[{"u", ",", "N"}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"left", "=", 
       RowBox[{"RotateLeft", "[", "u", "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"right", "=", 
       RowBox[{"RotateRight", "[", "u", "]"}]}], ";", "\[IndentingNewLine]", 
      "\[IndentingNewLine]", 
      RowBox[{"(*", " ", 
       RowBox[{"Target", " ", "cyclic", " ", "sum"}], " ", "*)"}], 
      "\[IndentingNewLine]", 
      RowBox[{"S", "=", 
       RowBox[{"Expand", "[", 
        RowBox[{"Total", "[", 
         RowBox[{"u", "*", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"right", "^", "2"}], "-", 
            RowBox[{"4", " ", 
             RowBox[{"u", "^", "2"}]}], "+", 
            RowBox[{"3", " ", 
             RowBox[{"left", "^", "2"}]}]}], ")"}]}], "]"}], "]"}]}], ";", 
      "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{"(*", " ", 
       RowBox[{
        RowBox[{"Edge", "-", 
         RowBox[{"wise", " ", "SOS", " ", "coefficients", " ", "for", " ", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{"u_i", " ", "-", " ", "u_", 
             RowBox[{"{", 
              RowBox[{"i", "+", "1"}], "}"}]}], ")"}], "^", "2"}], " ", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"c", " ", "u_i"}], " ", "+", " ", 
            RowBox[{"d", " ", "u_", 
             RowBox[{"{", 
              RowBox[{"i", "+", "1"}], "}"}]}]}], ")"}]}]}], " ", "*)"}], 
       "\[IndentingNewLine]", 
       RowBox[{"sol", "=", 
        RowBox[{"First", "[", 
         RowBox[{"FindEdgeSOSAnsatz", "[", 
          RowBox[{
           RowBox[{"-", "1"}], ",", 
           RowBox[{"-", "3"}]}], "]"}], "]"}]}], ";"}], " ", 
      RowBox[{"(*", " ", 
       RowBox[{"matches", " ", 
        RowBox[{"-", "u_i"}], "^", "2", " ", "u_", 
        RowBox[{"{", 
         RowBox[{"i", "+", "1"}], "}"}], " ", "-", " ", "3", " ", "u_i", " ", 
        "u_", 
        RowBox[{"{", 
         RowBox[{"i", "+", "1"}], "}"}], "^", "2"}], " ", "*)"}], 
      "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"a", ",", "b", ",", "c", ",", "d"}], "}"}], "=", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"a", ",", "b", ",", "c", ",", "d"}], "}"}], "/.", "sol"}]}],
       ";", "     ", 
      RowBox[{"(*", " ", 
       RowBox[{"Expected", ":", " ", 
        RowBox[{"a", "=", 
         RowBox[{"5", "/", "3"}]}], ",", " ", 
        RowBox[{"b", "=", 
         RowBox[{"7", "/", "3"}]}], ",", " ", 
        RowBox[{"c", "=", 
         RowBox[{"5", "/", "3"}]}], ",", " ", 
        RowBox[{"d", "=", 
         RowBox[{"7", "/", "3"}]}]}], " ", "*)"}], "\[IndentingNewLine]", 
      "\[IndentingNewLine]", 
      RowBox[{"(*", " ", 
       RowBox[{"Nonnegative", " ", "certificate"}], " ", "*)"}], 
      "\[IndentingNewLine]", 
      RowBox[{"P", "=", 
       RowBox[{"Expand", "[", 
        RowBox[{"Total", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"(", 
            RowBox[{"u", "-", "left"}], ")"}], "^", "2"}], "*", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"c", " ", "u"}], "+", 
            RowBox[{"d", " ", "left"}]}], ")"}]}], "]"}], "]"}]}], ";", 
      "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{"(*", " ", 
       RowBox[{"Identity", ":", " ", 
        RowBox[{"P", " ", "==", " ", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{"a", " ", "+", " ", "b"}], ")"}], " ", "Sum", " ", 
          RowBox[{"u_i", "^", "3"}], " ", "-", " ", "Sum", " ", "u_i", " ", 
          "u_", 
          RowBox[{"{", 
           RowBox[{"i", "-", "1"}], "}"}], "^", "2", " ", "-", " ", "3", " ", 
          "Sum", " ", "u_i", " ", "u_", 
          RowBox[{"{", 
           RowBox[{"i", "+", "1"}], "}"}], "^", "2"}]}]}], " ", "*)"}], 
      "\[IndentingNewLine]", 
      RowBox[{"idOK", "=", 
       RowBox[{
        RowBox[{"Simplify", "[", "\[IndentingNewLine]", 
         RowBox[{"Expand", "[", 
          RowBox[{"P", "-", 
           RowBox[{"(", 
            RowBox[{
             RowBox[{
              RowBox[{"(", 
               RowBox[{"a", "+", "b"}], ")"}], 
              RowBox[{"Total", "[", 
               RowBox[{"u", "^", "3"}], "]"}]}], "-", 
             RowBox[{"Total", "[", 
              RowBox[{"u", "*", 
               RowBox[{"right", "^", "2"}]}], "]"}], "-", 
             RowBox[{"3", " ", 
              RowBox[{"Total", "[", 
               RowBox[{"u", "*", 
                RowBox[{"left", "^", "2"}]}], "]"}]}]}], ")"}]}], "]"}], 
         "\[IndentingNewLine]", "]"}], "===", "0"}]}], ";", 
      "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{"(*", " ", 
       RowBox[{
       "Nonnegativity", " ", "and", " ", "conclusion", " ", "under", " ", 
        "u_i", " ", ">=", " ", "0"}], " ", "*)"}], "\[IndentingNewLine]", 
      RowBox[{"nonnegOK", "=", 
       RowBox[{"Simplify", "[", 
        RowBox[{
         RowBox[{"P", ">=", "0"}], ",", 
         RowBox[{"Assumptions", "->", 
          RowBox[{"And", "@@", 
           RowBox[{"Thread", "[", 
            RowBox[{"u", ">=", "0"}], "]"}]}]}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"concl", "=", 
       RowBox[{"Simplify", "[", 
        RowBox[{
         RowBox[{"S", "<=", "0"}], ",", 
         RowBox[{"Assumptions", "->", 
          RowBox[{"And", "@@", 
           RowBox[{"Thread", "[", 
            RowBox[{"u", ">=", "0"}], "]"}]}]}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{"Association", "[", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"\"\<IdentityHolds\>\"", "->", "idOK"}], ",", 
        "\[IndentingNewLine]", 
        RowBox[{"\"\<EdgeWeights\>\"", "->", 
         RowBox[{"Association", "[", 
          RowBox[{
           RowBox[{"\"\<a\>\"", "->", "a"}], ",", 
           RowBox[{"\"\<b\>\"", "->", "b"}], ",", 
           RowBox[{"\"\<c\>\"", "->", "c"}], ",", 
           RowBox[{"\"\<d\>\"", "->", "d"}]}], "]"}]}], ",", 
        "\[IndentingNewLine]", 
        RowBox[{"\"\<Certificate\>\"", "->", "P"}], ",", 
        "\[IndentingNewLine]", 
        RowBox[{"\"\<NonnegativityCertified\>\"", "->", "nonnegOK"}], ",", 
        "\[IndentingNewLine]", 
        RowBox[{"\"\<Conclusion\>\"", "->", "concl"}]}], 
       "\[IndentingNewLine]", "]"}]}]}], "\[IndentingNewLine]", "]"}]}], 
  ";"}]], "Input",
 InitializationCell->True],

Cell["Run the automated proof", "Section"],

Cell[BoxData[
 RowBox[{
  RowBox[{"N", "=", "10"}], ";", " ", 
  RowBox[{"(*", " ", 
   RowBox[{"choose", " ", "any", " ", "positive", " ", "integer"}], " ", 
   "*)"}], "\[IndentingNewLine]", 
  RowBox[{"res", "=", 
   RowBox[{"ProveExampleInequality", "[", "N", "]"}]}]}]], "Input"],

Cell["Interpretation of the result:", "Text"],

Cell[BoxData[
 RowBox[{"{", "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"\"\<Edge weights\>\"", "->", 
    RowBox[{"res", "[", "\"\<EdgeWeights\>\"", "]"}]}], ",", 
   "\[IndentingNewLine]", 
   RowBox[{"\"\<Identity holds?\>\"", "->", 
    RowBox[{"res", "[", "\"\<IdentityHolds\>\"", "]"}]}], ",", 
   "\[IndentingNewLine]", 
   RowBox[{
   "\"\<Certificate is nonnegative under u >= 0?\>\"", "->", 
    RowBox[{"res", "[", "\"\<NonnegativityCertified\>\"", "]"}]}], ",", 
   "\[IndentingNewLine]", 
   RowBox[{"\"\<Conclusion S <= 0 under u >= 0?\>\"", "->", 
    RowBox[{"res", "[", "\"\<Conclusion\>\"", "]"}]}]}], 
  "\[IndentingNewLine]", "}"}]], "Input"],

Cell["\<\
Inspect the nonnegative certificate (sum of edge-wise terms):\
\>", "Text"],

Cell[BoxData[
 RowBox[{"TraditionalForm", "[", 
  RowBox[{"res", "[", "\"\<Certificate\>\"", "]"}], "]"}]], "Input"],

Cell["\<\
Optional: numeric sanity check with random nonnegative vectors\
\>", "Section"],

Cell[BoxData[
 RowBox[{"N", "[", "\[IndentingNewLine]", 
  RowBox[{"With", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"trial", "=", "5"}], ",", 
      RowBox[{"N", "=", "12"}]}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{"Table", "[", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"Module", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{
          RowBox[{"u", "=", 
           RowBox[{"RandomReal", "[", 
            RowBox[{
             RowBox[{"{", 
              RowBox[{"0", ",", "2"}], "}"}], ",", "N"}], "]"}]}], ",", 
          "left", ",", "right", ",", "S"}], "}"}], ",", 
        "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"left", "=", 
          RowBox[{"RotateLeft", "[", "u", "]"}]}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"right", "=", 
          RowBox[{"RotateRight", "[", "u", "]"}]}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"S", "=", 
          RowBox[{"Total", "[", 
           RowBox[{"u", "*", 
            RowBox[{"(", 
             RowBox[{
              RowBox[{"right", "^", "2"}], "-", 
              RowBox[{"4", " ", 
               RowBox[{"u", "^", "2"}]}], "+", 
              RowBox[{"3", " ", 
               RowBox[{"left", "^", "2"}]}]}], ")"}]}], "]"}]}], ";", 
         "\[IndentingNewLine]", "S"}]}], "\[IndentingNewLine]", "]"}], ",", 
      "\[IndentingNewLine]", 
      RowBox[{"{", "trial", "}"}]}], "]"}]}], "\[IndentingNewLine]", "]"}], 
  "\[IndentingNewLine]", "]"}]], "Input"],

Cell["Notes", "Section"],

Cell["\<\
The proof is based on the edge-wise inequality (u_i - u_{i+1})^2 ( (5/3) u_i \
+ (7/3) u_{i+1}) >= 0, whose telescoped sum equals 4 Sum u_i^3 - Sum u_i \
u_{i-1}^2 - 3 Sum u_i u_{i+1}^2, yielding the target S <= 0.\
\>", "Text"]
